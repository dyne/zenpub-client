{
  "name": "zenpub",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://os.reflowproject.eu/",
  "scripts": {
    "start": "cross-env NODE_ENV=development react-scripts start --incremental",
    "build": "lingui extract --clean ; lingui compile ; cross-env NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-as-is": "cross-env NODE_ENV=production node scripts/build.js",
    "preinstall": "npx use-yarn",
    "precommit": "lint-staged",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract --clean",
    "compile": "lingui compile",
    "docs": "typedoc --out docs src",
    "storybook": "start-storybook -p 5000 --docs",
    "build-storybook": "build-storybook --docs -c .storybook",
    "build-deploy-storybook": "./storybook_deploy.sh",
    "gqlcode": "node ./codegen",
    "gql:fetch-schema": "graphql get-schema --project",
    "lint-watch": "esw --clear -w \"src/**/!(*.generated).{ts,tsx}\" --ext .tsx --ext .ts --changed"
  },
  "lint-staged": {
    "src/**/!(*.generated).{js,jsx,ts,tsx,json,css,md}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "macros",
      "async-import"
    ],
    "presets": [
      "react-app",
      "@lingui/babel-preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error"
    }
  },
